{
  "name": "studio-macros",
  "version": "1.0.0",
  "description": "macros to automatically export DAW sessions",
  "main": "main.js",
  "scripts": {
    "react-start": "webpack serve --mode development --env development",
    "electron-start": "electron .",
    "start": "concurrently \"cross-env NODE_ENV=development npm run react-start\" \"wait-on http://localhost:8080 && cross-env NODE_ENV=development npm run electron-start\"",
    "build": "cross-env NODE_ENV=production webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "studio-macros"
  },
  "keywords": [
    "music"
  ],
  "author": "Patrick Carter",
  "license": "ISC",
  "dependencies": {
    "@nut-tree/nut-js": "^3.1.1",
    "autoprefixer": "^10.4.14",
    "electron": "^24.3.0",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "screenshot-desktop": "^0.0.0-development",
    "tailwindcss": "^3.3.2",
    "tesseract.js": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "electron-reload": "^2.0.0-alpha.1",
    "html-webpack-plugin": "^5.5.1",
    "style-loader": "^3.3.2",
    "wait-on": "^7.0.1",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  }
}
